<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Alfresco</Name>
  <Beta>False</Beta>
<Category>Productivity:</Category>
  <Overview>Document management system</Overview>
  <Support>http://lime-technology.com/forum/</Support>
  <Description></Description>
  <Registry>https://registry.hub.docker.com/u/gui81/alfresco</Registry>
  <Repository>gui81/alfresco</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name></Name>
      <Value></Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>host</Mode>
    <Publish>
	  <Port>
        <HostPort>8080</HostPort>
        <ContainerPort>8080</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
   <Volume>
      <HostDir></HostDir>
      <ContainerDir>/alfresco/alf_data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
      <Volume>
      <HostDir></HostDir>
      <ContainerDir>/alfresco/tomcat/logs</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8080]/</WebUI>
  <Banner>https://cdn.evbuc.com/eventlogos/1509557/alfrescologotransparentbackground5bconverted5d.png</Banner>
  <Icon>https://www.alfresco.com/sites/www.alfresco.com/files/alfresco-logo.png</Icon>
</Container>
