<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Name>Flickr-uploader</Name>
  <Beta>True</Beta>
  <Category>Productivity:</Category>
  <Date>2015-10-24</Date>
  <Overview>Upload pictures to flicker</Overview>
  <Description></Description>
  <Registry>https://registry.hub.docker.com/u/snoopy86/flickr-uploader</Registry>
  <Repository>snoopy86/flickr-uploader</Repository>
  <BindTime>true</BindTime>
  <Privileged>false</Privileged>
  <Environment>
    <Variable>
      <Name>PYTHONUNBUFFERED</Name>
      <Value>0</Value>
    </Variable>
  </Environment>
  <Networking>
    <Mode>host</Mode>
  </Networking>
  <Data>
   <Volume>
      <HostDir></HostDir>
      <ContainerDir>/pictures</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
      <Volume>
      <HostDir></HostDir>
      <ContainerDir>/folders2flickr</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI></WebUI>
  <Banner>http://www.winona.edu/Images/flickr_banner-519.jpg</Banner>
  <Icon>http://acuril.uprrp.edu/images/Images/logos/flickr.png</Icon>
</Container>
